<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<record id="pr_reject_form" model="ir.ui.view">
			<field name="name">skit.pr.reject.wizard.form</field>
			<field name="model">skit.pr.reject.wizard</field>
			<field name="arch" type="xml">
				<form string="Reject Approval" edit="true">
					<group>
						<group>
							<field name="comment" readonly="0" nolabel="1" required="1"/>
						</group>
					</group>
					<footer>
                        <button string='Reject' name="reject_approval" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
				</form>
			</field>
		</record>
	</data>
</odoo>